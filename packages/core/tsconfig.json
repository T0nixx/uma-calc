{
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "dist",
    "module": "ESNext",
    "declaration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "removeComments": true,
    "noImplicitAny": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true
  }
}
